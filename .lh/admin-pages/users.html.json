            "date": 1766571412475,
            "name": "Commit-0",
            "content": "<!DOCTYPE html>\n<html lang=\"fr\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <!-- font awsome -->\n<script src=\"https://kit.fontawesome.com/f14d152ebc.js\" crossorigin=\"anonymous\"></script>\n  <!-- google font -->\n      <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\">\n      <!-- font family -->\n<link href=\"https://fonts.googleapis.com/css2?family=Cairo:wght@200..1000&family=Open+Sans:ital,wght@0,300..800;1,300..800&family=Work+Sans:ital,wght@0,100..900;1,100..900&display=swap\" rel=\"stylesheet\">\n    <!-- style -->\n   <link rel=\"stylesheet\" href=\"assets/message.css\">\n    <title>Formix Admin Messages</title>\n</head>\n<body>\n    <div class=\"message\">\n       \n<aside class=\"side-bar\">\n  <h3>Formix</h3>\n  <ul>\n    <li>\n      <a href=\"dashboard.html\">\n        <i class=\"fa-solid fa-gauge-high fa-fw\"></i>\n        <span>Dashboard</span>\n      </a>\n    </li>\n\n    <li>\n      <a href=\"users.html\">\n        <i class=\"fa-solid fa-users fa-fw\"></i>\n        <span>Users</span>\n      </a>\n    </li>\n\n    <li>\n      <a href=\"instructors.html\">\n        <i class=\"fa-solid fa-chalkboard-user fa-fw\"></i>\n        <span>Instructors</span>\n      </a>\n    </li>\n\n   \n\n     <li>\n      <a href=\"orders.html\">\n    <i class=\"fa-solid fa-cart-shopping\"></i>\n\n        <span>Orders</span>\n      </a>\n    </li>\n    <li>\n      <a href=\"Reviews.html\">\n<i class=\"fa-regular fa-star\"></i>\n  <span>Reviews</span>\n      </a>\n    </li>\n\n    <li>\n      <a href=\"messages.html\">\n        <i class=\"fa-solid fa-comments fa-fw\"></i>\n        <span>Messages</span>\n      </a>\n    </li>\n\n    <li>\n      <a href=\"settings.html\">\n        <i class=\"fa-solid fa-gear fa-fw\"></i>\n        <span>Settings</span>\n      </a>\n    </li>\n  </ul>\n</aside>\n\n        \n        <div class=\"content\">\n            <header class=\"admin-header\">\n  <div class=\"search-box\">\n    <input type=\"text\" placeholder=\"Search...\">\n    <i class=\"fa-solid fa-magnifying-glass search-icon\"></i>\n  </div>\n\n  <div class=\"header-right\">\n    <button class=\"notification-btn\">\n      <i class=\"fa-solid fa-bell\"></i>\n      <span class=\"notif-count\">3</span>\n    </button>\n    <div class=\"profile\">\n      <img src=\"../assets/images/testimonials/testi1.png\" alt=\"Profile\">\n    </div>\n  </div>\n</header>\n\n\n            <section>\n<div class=\"main-title\">\n    <h2>Messages </h2>\n    <p>Manage contact form submissions and user messages.\n\n\n</p>\n</div>\n<div class=\"messages-wrapper\">\n\n  <!-- Messages List -->\n  <div class=\"messages-list\">\n<button class=\"btn-card \"\n  data-reply=\"Our support team will review your request and get back to you as soon as possible. If you have any additional questions in the meantime, please feel free to contact us.\"\n   data-name=\"John Smith\"\n        data-email=\"john@example.com\"\n        data-date=\"2024-12-15\"\n  >\n  \n  <div class=\"message-card\">\n    <div class=\"msg-header\">\n      <h4>John Smith</h4>\n      <span class=\"badge new\">New</span>\n    </div>\n    <p>Course Refund Request</p>\n    <span class=\"date\">2024-12-15</span>\n  </div>\n</button>\n\n<button class=\"btn-card \" \n  data-reply=\"Thank you for reaching out to us. We've received your message regarding Technical Issue with Course Videos. Our support team will review your request and get back\n   to you as soon as possible. If you have any additional questions in the meantime, please feel free to contact us.\"\n    data-name=\"Sarah Johnson\"\n        data-email=\"Sarah Johnson@example.com\"\n        data-date=\"2024-12-14\">\n\n\n  <div class=\"message-card\">\n    <h4>Sarah Johnson</h4>\n    <p>Technical Issue with Course Videos</p>\n    <span class=\"date\">2024-12-14</span>\n  </div>\n</button>\n<button class=\"btn-card \"  \n data-reply=\"Thank you for reaching out to us. We've received your message regarding Technical Issue with Course Videos.\n  Our support team will review your request and get back to you as soon as possible. If you have any additional \n  questions in the meantime, please feel free to contact us.\"\n   data-name=\"Mike Davis\"\n        data-email=\"Mike Davis@example.com\"\n        data-date=\"2024-12-13\">\n  \n\n  <div class=\"message-card active\">\n    <div class=\"msg-header\">\n      <h4>Mike Davis</h4>\n      <span class=\"badge new\">New</span>\n    </div>\n    <p>Certificate Request</p>\n    <span class=\"date\">2024-12-13</span>\n  </div>\n</button>\n<button class=\"btn-card \"  \n data-reply=\"Thank you for reaching out to us. We've received your message regarding Instructor Application. Our support team will review your request and get back to you as soon as possible. \n If you have any additional questions in the meantime, please feel free to contact us.\"\n  data-name=\"Emma Wilson\"\n        data-email=\"Emma Wilson@example.com\"\n        data-date=\"2024-12-12\">\n\n  <div class=\"message-card\">\n    <h4>Emma Wilson</h4>\n    <p>Instructor Application</p>\n    <span class=\"date\">2024-12-12</span>\n  </div>\n\n</button>\n\n  </div>\n\n  <!-- Message Content -->\n  <div class=\"message-content-box \">\n<i class=\"fa-solid fa-message\"></i>\n\n    <span>Select a message to view details</span>\n\n  </div>\n\n</div>\n\n            </section>\n        </div>\n    </div>\n\n\n\n\n<script>\nconst buttons = document.querySelectorAll('.btn-card');\nconst box = document.querySelector('.message-content-box');\n\nbuttons.forEach(btn => {\n  btn.addEventListener('click', () => {\n\n    box.innerHTML = '';\n\n    // إنشاء العنصر للعرض\n    const messageDiv = document.createElement('div');\n    messageDiv.className = 'message-card no-border';\n\n    // إضافة البيانات من data attributes\n    const nameElem = document.createElement('h4');\n    nameElem.innerText = ` ${btn.dataset.name}`;\n\n    const emailElem = document.createElement('p');\n    emailElem.innerText = `Email: ${btn.dataset.email}`;\n\n    const dateElem = document.createElement('span');\n    dateElem.className = 'date';\n    dateElem.innerText = `Date: ${btn.dataset.date}`;\n\n    messageDiv.appendChild(nameElem);\n    messageDiv.appendChild(emailElem);\n    messageDiv.appendChild(dateElem);\n\n    box.appendChild(messageDiv);\n\n   \n    const replyDiv = document.createElement('div');\n    replyDiv.className = 'admin-reply';\n    replyDiv.innerHTML = `\n      <p>${btn.dataset.reply}</p>\n      <div class=\"reply-actions\">\n        <button class=\"mark-read\"><i class=\"fa-solid fa-message\"></i> Mark as read</button>\n        <button class=\"delete\"><i class=\"fa-solid fa-trash\"></i> Delete</button>\n      </div>\n    `;\n    box.appendChild(replyDiv);\n\n    box.classList.add('active');\n  });\n});\n\n</script>\n\n\n\n   \n\n</body>\n</html>"
        }
    ]
}